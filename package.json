{
  "name": "tale-botolf",
  "bin": "index.js",
  "version": "1.3.0",
  "description": "AI-drevet tekst til tale generator for kreativ, informativ og n√∏yaktig kommunikasjon.",
  "main": "index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tale_botolf",
            "iconUrl": "https://tfkweb.no/wp-content/uploads/2023/03/icon2.ico",
            "setupIcon": "assets/icon.ico"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "assets/icon.icns"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "assets/icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "assets/icon.png"
          }
        }
      ]
    }
  },
  "pkg": {
    "assets": [
      "views/**/*"
    ],
    "output": "dist"
  },
  "keywords": [],
  "author": "tfk",
  "license": "AGPL",
  "dependencies": {
    "app-root-path": "^3.1.0",
    "dotenv": "^16.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.25.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.0",
    "@electron-forge/maker-deb": "^6.1.0",
    "@electron-forge/maker-dmg": "^6.1.0",
    "@electron-forge/maker-rpm": "^6.1.0",
    "@electron-forge/maker-squirrel": "^6.1.0",
    "@electron-forge/maker-zip": "^6.1.0",
    "electron": "^23.1.3",
    "electron-forge": "^5.2.4",
    "reinstall-node-modules": "^1.2.0"
  },
  "build": {
    "appId": "no.tfkweb.tale-botolf",
    "productName": "Tale-Botolf",
    "publish": [
      {
        "provider": "github",
        "owner": "Trondfk90",
        "repo": "tale-app"
      }
    ]
  }
}
